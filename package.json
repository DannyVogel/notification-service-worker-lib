{
  "name": "notification-service-worker-lib",
  "private": false,
  "description": "A library for sending notifications to the service worker",
  "version": "0.0.3",
  "type": "module",
  "main": "dist/notification-service-worker.cjs",
  "module": "dist/notification-service-worker.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
